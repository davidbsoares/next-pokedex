// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Pokemon {
  id          Int    @id
  name        String
  weight      Int
  height      Int
  kinds       String
  types       String
  hp          Int
  atk         Int
  def         Int
  satk        Int
  sdef        Int
  spd         Int
  moves       String
  image       String
  description String
}
